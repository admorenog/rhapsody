{
	"name": "rhapsody",
	"version": "0.4.5",
	"description": "electron framework",
	"main": "app/core/app.js",
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"start": "electron .",
		"build:quick": "electron-builder --dir",
		"release:this": "electron-builder",
		"release:mac": "electron-builder --mac",
		"release:win": "electron-builder --win",
		"release:linux": "electron-builder --linux"
	},
	"repository": {
		"type": "git",
		"url": "git://git@raspi/fqb.git"
	},
	"license": "MIT",
	"keywords": [],
	"author": "amg",
	"dependencies": {
		"@types/ejs": "^2.6.3",
		"bootstrap": "^4.3.1",
		"ejs": "^2.5.6",
		"haml": "^0.4.3",
		"pug": "^2.0.0-rc.2",
		"sqlite3": "^4.0.8",
		"typescript": "^3.4.5",
		"node-notifier": "^5.4.0"
	},
	"devDependencies": {
		"@types/node": "^11.13.10",
		"@types/sqlite3": "^3.1.5",
		"bootstrap-sass": "^3.4.1",
		"del": "^4.1.1",
		"electron": "^4.2.0",
		"electron-builder": "^20.39.0",
		"electron-rebuild": "^1.8.4",
		"gulp": "^4.0.1",
		"gulp-livereload": "^4.0.1",
		"gulp-sass": "^4.0.2",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^5.0.1",
		"jquery": "^3.4.1",
		"node-sass": "^4.12.0",
		"notify-send": "^0.1.2",
		"popper.js": "^1.15.0"
	},
	"build": {
		"appId": "amg.rhapsody",
		"mac": {
			"category": "framework"
		},
		"extraFiles": [
			".env",
			"config/**/*"
		],
		"asar": "true"
	}
}
